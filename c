//代码1
#include <stdio.h>
void main()
{
	int age = 0;

	if (age < 18)
	{
		printf("未成年\n");
	}
	return 0;
}
//代码2
#include <stdio.h>
int main()
{
	int age = 20;

	if (age < 18)
	{
		printf("未成年\n");
	}
	else
	{
		printf("成年\n");
	}
}

//代码3
#include <stdio.h>
int main()
{
	int age = 18;

	if (age < 18)
	{
		printf("少年\n");
	}
	else if (age >= 18 && age < 30)
	{
		printf("青年\n");
	}
	else if (age >= 30 && age < 50)
	{
		printf("中年\n");
	}
	else (age >= 50 && age < 80)
	{
		printf("老年\n");
	}
}
